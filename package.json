{
  "name": "webpro-backend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node build/server.js",
    "dev": "nodemon --exec babel-node src/server.js",
    "pm:build": "prisma generate",
    "pm:pull": "prisma db pull",
    "pm:push": "prisma db push",
    "test": "dotenv -e .env.test -- yarn test:dev && yarn test:lint",
    "test:dev": "jest",
    "test:lint": "eslint .",
    "build": "yarn clean && yarn build:js",
    "build:js": "babel ./src -d ./build --copy-files",
    "clean": "rimraf ./build"
  },
  "dependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.9",
    "@babel/preset-env": "^7.16.11",
    "@prisma/client": "^3.12.0",
    "body-parser": "^1.20.0",
    "cli-color": "^2.0.2",
    "core-js": "^3.21.1",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "nodemailer": "^6.7.3",
    "regenerator-runtime": "^0.13.9",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@babel/node": "^7.16.8",
    "dotenv-cli": "^5.1.0",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^26.1.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "jest": "^27.5.1",
    "nodemon": "^2.0.15",
    "prisma": "^3.12.0"
  }
}
